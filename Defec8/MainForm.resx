  <data name="fctbCode.Text" xml:space="preserve">
    <value>width equ 160
height equ 120
wh equ 19200
gradLevel equ 30
gradLevelmh2 equ -15
gradLeveldb equ 8
smoothHiBoundary equ 224

movi sp, 0x10000
movi cx, wh
loop: addi cx, -1
call drawpixel
cmpi cx, 0
jnz loop
hlt

drawpixel: movi si, 0x40000
add si, cx
mov cx, ax
mov cx, bx
modi ax, width
modi ax, gradLevel
addi ax, gradLevelmh2
abs ax
muli ax, gradLeveldb
divi bx, width
modi bx, gradLevel
addi bx, gradLevelmh2
abs bx
muli bx, gradLeveldb
add ax, bx
call smoothstep
call smoothstep
call smoothstep
call smoothstep
rpokeb ax, si
ret

smoothstep: fmovrf ax, ax
fmovi bx, smoothHiBoundary
fdiv ax, bx
mov ax, bx
fmul bx, bx
fmul ax, bx
fmovi di, 3
fmul bx, di
fmovi di, 2
fmul ax, di
fsub bx, ax
fmovi ax, smoothHiBoundary
fmul ax, bx
fmovfr ax, ax
ret </value>
